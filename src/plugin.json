{
    "$schema": "https://raw.githubusercontent.com/grafana/grafana/main/docs/sources/developers/plugins/plugin.schema.json",
    "type": "app",
    "name": "Grafana-Yamcs Integration",
    "id": "jaops-yamcs-app",
    "info": {
        "keywords": ["app"],
        "description": "",
        "author": {
            "name": "JAOPS"
        },
        "logos": {
            "small": "img/logo.png",
            "large": "img/logo.png"
        },
        "screenshots": [],
        "version": "%VERSION%",
        "updated": "%TODAY%"
    },
    "includes": [
        {
            "type": "page",
            "name": "Overview",
            "path": "/a/%PLUGIN_ID%/",
            "addToNav": true,
            "defaultNav": true
        },
        {
            "type": "page",
            "name": "How to use the plugin",
            "path": "/a/%PLUGIN_ID%/how-to-use",
            "icon": "question-circle",
            "addToNav": true
        },
        {
            "type": "page",
            "name": "Commanding",
            "icon": "rocket",
            "path": "/a/%PLUGIN_ID%/commanding-setup",
            "addToNav": true
        },
        {
            "type": "page",
            "name": "Image Panel",
            "icon": "gf-portrait",
            "path": "/a/%PLUGIN_ID%/image-panel-setup",
            "addToNav": true
        },
        {
            "type": "page",
            "name": "Variables",
            "icon": "variable",
            "path": "/a/%PLUGIN_ID%/variable-setup",
            "addToNav": true
        }
    ],
    "dependencies": {
        "grafanaDependency": ">=10.4.0",
        "plugins": []
    }
}
